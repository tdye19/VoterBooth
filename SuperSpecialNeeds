package VotingBooth;

public class SuperSpecialNeeds extends Voter {
	
	@Override
    public void setLeaveTime(int leaveTime) {
        this.leaveTime = (leaveTime * 4);
    }

	
	public void setVotingBoothTime(int votingBoothTime) {
		this.boothTime = votingBoothTime * 3;
		
	}

	@Override
	public void setCheckInTime(double checkInTime) {
		this.checkInTime = checkInTime * 4;
		
	}
}
