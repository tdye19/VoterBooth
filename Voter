package VoterBooth;
/**
 * @author   Roger Ferguson
 */
	public abstract class Voter{
		
	private int tickTime;
	private Booth Destination;
	
	protected int leaveTime;
	protected int checkInTime;
	protected int votingBoothTime;
	protected double boothTime;
	
	public abstract void setCheckInTime(int checkInTime);
	public abstract void setLeaveTime(int leaveTime);
	public abstract void setVotingBoothTime(int votingBoothTime);
	
	public double getBoothTime() {
		return boothTime;
	}
	
	public void setBoothTime(double checkInTime) {
		this.boothTime = checkInTime;
	}
	
	public Booth getDestination() {
		return Destination;
	}

	public void setDestination(Booth destination) {
		this.Destination = destination;
	}
	
	public int getTickTime() {
		return tickTime;
	}

	public void setTickTime(int tickTime) {
		this.tickTime = tickTime;
	}
}
