package VotingBooth;

public class LimitedTimeVoter extends Voter{

	@Override
	public void setCheckInTime(double checkInTime) {
		this.checkInTime = checkInTime;
	}

	@Override
	public void setLeaveTime(int leaveTime) {
		this.leaveTime = (int) (leaveTime * 0.5);
	}
	public void setVotingBoothTime(int votingBoothTime) {
		this.boothTime = (int) (votingBoothTime * 0.5);
		
	}

}
